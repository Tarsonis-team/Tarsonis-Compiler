type Point is record
    var x: integer
    var y: integer
end

type Line is record
    var begin: Point
    var wend: Point
end

routine new_point(integer x, integer y) -> Point is
    var p: Point
    p.x := x
    p.y := y

end

routine new_line(Point p1, Point p2) -> Line is
    var pf: Pointf
    pf.begin := p1
    pf.ende := p2

end
