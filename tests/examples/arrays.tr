
type Point is record
    var x: integer
    var y: integer
end

type Array is array[4] integer;

routine two(integer a) -> integer is
    if a > 3 then
        var c: integer;
        return c;
        var l : real;
        return l;
    end;
end

routine main() is
    var a: array[2] Point;
    var b: array[2] array[3] integer;

    b[0][1] := two(2);
end
