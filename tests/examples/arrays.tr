
type Point is record
    var x: integer
    var y: integer
end

type SockFD is integer;
type P is Point;

routine two(SockFD a) -> SockFD is
    if a > 3 then
        var c: integer;
        return c;
        var l : real;
        return l;
    end;
end

routine main() -> SockFD is
    var a: array[2] P;
    var b: array[2] array[3] P;

    b[0][1].x := two(2) + b[0][0].x;

    return b[0][0].x + a[0].y;
end
