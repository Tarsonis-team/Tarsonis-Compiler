
type Point is record
    var x: integer
    var y: integer
end

type SockFD is integer;
type P is Point;

routine two(integer a) -> P is
    var p: P;
    return p;
    if a > 3 then
        var c: Point;
        return c;
        var l : Point;
        return l;
    else 
        var c: P;
    end
end

routine main() -> SockFD is
    var a: array[2] Point;
    var b: array[2] array[3] P;

    b[0][1].x := b[0][0].y;
    b[0][0] := two(b[0][0].y);
    
    two(1);

    return b[0][0].x
end
